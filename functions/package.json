{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "start-clean": "npx kill-port --port 9099,5001,8181,5000,9199,4001,4500,4400,4001 && firebase emulators:start --import='./dev/firebase-emulator/' --export-on-exit='./dev/firebase-emulator/'",
    "start-serve": "firebase emulators:start --import='./dev/firebase-emulator/' --export-on-exit='./dev/firebase-emulator/'",
    "stop-serve": "npx kill-port --port 9099,5001,8181,5000,9199,4001,4500,4400,4001",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
